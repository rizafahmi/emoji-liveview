<dialog open={assigns[:show_dialog]} id="feedback-dialog">
  <.form for={@form} id="feedback-form" phx-submit="submit">
    <.input field={@form[:emoji]} type="text" label="Emoji" />
    <button type="submit">Submit</button>
  </.form>
</dialog>

<div id="emoji" :for={feedback <- @feedbacks} style={"position: absolute; left: #{Enum.random(0..100)}%; top: #{Enum.random(0..100)}%;"}>
<span><%= feedback.emoji %></span>
</div>
